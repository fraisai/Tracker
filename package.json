{
  "name": "trackr",
  "version": "1.0.0",
  "description": "Job application tracking tool and dashboard deployed in a Kubernetes cluster on Amazon EKS.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "start": "node ./server/server.js",
    "dev": "NODE_ENV=development nodemon -r esm ./server/server.js & webpack-dev-server --open --hot",
    "docker-dev:hot": "docker-compose -f docker-compose-dev-hot.yml up",
    "create-cluster": "bash ./src/eks-shell/create-cluster.sh",
    "delete-cluster": "bash ./src/eks-shell/delete-cluster.sh",
    "check-user": "bash ./src/eks-shell/check-aws-user.sh",
    "check-resources": "bash ./src/eks-shell/view-namespace-resources.sh",
    "apply-deployment": "bash ./src/eks-shell/apply-deployment.sh",
    "apply-service": "bash ./src/eks-shell/apply-service.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "node-kill": "sudo killall -9 node",
    "url-ui": "kubectl get svc ui"
  },
  "author": "F. Raisa Iftekher",
  "license": "ISC",
  "keywords": [],
  "dependencies": {
    "axios": "^1.6.7",
    "buffer-shims": "^1.0.0",
    "core-util-is": "^1.0.3",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@node-loader/babel": "^2.1.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "react-router-dom": "^6.22.3",
    "ret": "^0.5.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  }
}
